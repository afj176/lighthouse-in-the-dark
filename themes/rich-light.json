	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#5A5A5A",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": ["string", "string.quoted", "string.template"],
			"settings": {
				"foreground": "#7A4A1F"
			}
		},
		{
			"name": "String Escape",
			"scope": ["constant.character.escape", "string.regexp"],
			"settings": {
				"foreground": "#A85A2F"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#8B2EB8"
			}
		},
		{
			"name": "Keyword Control",
			"scope": ["keyword.control", "keyword.control.flow", "keyword.control.import", "keyword.control.export"],
			"settings": {
				"foreground": "#8B2EB8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operator",
			"scope": ["keyword.operator", "keyword.operator.assignment", "keyword.operator.arithmetic", "keyword.operator.logical"],
			"settings": {
				"foreground": "#B8266A"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable", "variable.other"],
			"settings": {
				"foreground": "#A83D45"
			}
		},
		{
			"name": "Variable Parameter",
			"scope": ["variable.parameter", "meta.function.parameters"],
			"settings": {
				"foreground": "#8B5A30",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variable Language",
			"scope": ["variable.language", "variable.language.this", "variable.language.self"],
			"settings": {
				"foreground": "#B8266A",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variable Property",
			"scope": ["variable.other.property", "variable.other.object.property"],
			"settings": {
				"foreground": "#6B4A28"
			}
		},
		{
			"name": "Constant",
			"scope": ["constant", "constant.language", "constant.language.boolean"],
			"settings": {
				"foreground": "#6B5A10"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric", "constant.numeric.integer", "constant.numeric.float", "constant.numeric.hex"],
			"settings": {
				"foreground": "#6B5A10"
			}
		},
		{
			"name": "Function",
			"scope": ["entity.name.function", "meta.function-call", "support.function"],
			"settings": {
				"foreground": "#A83D45"
			}
		},
		{
			"name": "Function Declaration",
			"scope": ["entity.name.function.definition", "meta.function entity.name.function"],
			"settings": {
				"foreground": "#8B2A35",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Class",
			"scope": ["entity.name.class", "entity.name.type.class", "support.class"],
			"settings": {
				"foreground": "#6B5A10",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Type",
			"scope": ["entity.name.type", "support.type", "entity.name.type.interface", "entity.name.type.alias"],
			"settings": {
				"foreground": "#2E6B8A"
			}
		},
		{
			"name": "Namespace",
			"scope": ["entity.name.namespace", "entity.name.type.module"],
			"settings": {
				"foreground": "#2E6B8A"
			}
		},
		{
			"name": "Object Key",
			"scope": ["meta.object-literal.key", "support.type.property-name.json"],
			"settings": {
				"foreground": "#8B5A30"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation", "punctuation.separator", "punctuation.terminator", "punctuation.definition"],
			"settings": {
				"foreground": "#6B5A45"
			}
		},
		{
			"name": "Punctuation Brackets",
			"scope": ["punctuation.definition.block", "punctuation.section", "meta.brace"],
			"settings": {
				"foreground": "#5A4A38"
			}
		},
		{
			"name": "HTML Tag",
			"scope": ["entity.name.tag", "punctuation.definition.tag"],
			"settings": {
				"foreground": "#A83D45"
			}
		},
		{
			"name": "HTML Attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#8B5A30",
				"fontStyle": "italic"
			}
		},
		{
			"name": "CSS Property",
			"scope": ["support.type.property-name.css", "meta.property-name.css"],
			"settings": {
				"foreground": "#2E6B8A"
			}
		},
		{
			"name": "CSS Value",
			"scope": ["support.constant.property-value.css", "meta.property-value.css"],
			"settings": {
				"foreground": "#7A4A1F"
			}
		},
		{
			"name": "CSS Selector",
			"scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
			"settings": {
				"foreground": "#8B2EB8"
			}
		},
		{
			"name": "Markup Heading",
			"scope": ["markup.heading", "entity.name.section.markdown"],
			"settings": {
				"foreground": "#8B2EB8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup Bold",
			"scope": ["markup.bold"],
			"settings": {
				"foreground": "#A83D45",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup Italic",
			"scope": ["markup.italic"],
			"settings": {
				"foreground": "#A83D45",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup Link",
			"scope": ["markup.underline.link", "string.other.link"],
			"settings": {
				"foreground": "#2E6B8A",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Code",
			"scope": ["markup.inline.raw", "markup.fenced_code", "markup.raw.block"],
			"settings": {
				"foreground": "#7A4A1F"
			}
		},
		{
			"name": "Markup Quote",
			"scope": ["markup.quote"],
			"settings": {
				"foreground": "#5A5A5A",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup List",
			"scope": ["markup.list", "punctuation.definition.list"],
			"settings": {
				"foreground": "#B8266A"
			}
		},
		{
			"name": "Storage",
			"scope": ["storage", "storage.type.function", "storage.type.class"],
			"settings": {
				"foreground": "#8B2EB8"
			}
		},
		{
			"name": "Support",
			"scope": ["support", "support.constant", "support.variable"],
			"settings": {
				"foreground": "#2E6B8A"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FFFFFF",
				"background": "#A83D45"
			}
		},
		{
			"name": "Deprecated",
			"scope": ["invalid.deprecated"],
			"settings": {
				"foreground": "#FFFFFF",
				"background": "#CC5C2A"
			}
		}
	]
